<li class="list__item">
    <a class="list__link" href="{{ block.channelAddress }}" title="{{ footer.companyName }} | {{ block.channel }}"
       target="_blank" rel="noreferrer">
        {% switch block.channel %}

        {% case 'facebook' %}
            <svg class="icon icon--20">
                <use xlink:href="{{ rev('/app/assets/img/symbols.svg') }}#facebook"></use>
            </svg>

        {% case 'instagram' %}
            <svg class="icon icon--20">
                <use xlink:href="{{ rev('/app/assets/img/symbols.svg') }}#instagram"></use>
            </svg>

        {% case 'linkedin' %}
            <svg class="icon icon--20">
                <use xlink:href="{{ rev('/app/assets/img/symbols.svg') }}#linkedin"></use>
            </svg>

        {% case 'youtube' %}
            <svg class="icon icon--20">
                <use xlink:href="{{ rev('/app/assets/img/symbols.svg') }}#youtube"></use>
            </svg>

        {% endswitch %}
    </a>
</li>
